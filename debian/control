 libcurl4-gnutls-dev (>= 7.10) | libcurl-dev (>= 7.10),
 libdbus-1-dev,
 libfaad-dev,
 libflac-dev,
 libgtk2.0-dev (>= 2.12),
 libgtk-3-dev,
 libimlib2-dev,
